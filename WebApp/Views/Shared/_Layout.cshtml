                  @if(User.Identity != null && User.Identity.IsAuthenticated){
                    <li><a href="#" class="nav-link">Contact</a></li>     
                    @if(User.IsInRole("2")){
                      <li><a href="/car/add" class="nav-link">Đăng ký xe</a></li>
                      <li><a href="/car" class="nav-link">Xe của tôi</a></li>
                      <li><a href="#" class="nav-link">Chuyến đi</a></li>                    
                    }    
                    @if(User.IsInRole("3")){                      
                      <li><a href="#" class="nav-link">Chuyến đi</a></li>
                    }                    
                    <li><a href="/auth/logout" class="nav-link">Đăng xuất</a></li>
                  } else{
                    <li><a href="/auth/login" class="nav-link">Đăng nhập</a></li>
                    <li><a href="/auth/register" class="nav-link">Đăng ký</a></li>                    
                  }                   
